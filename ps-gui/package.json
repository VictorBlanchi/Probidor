{
  "private": true,
  "devDependencies": {
    "parcel": "2.7.0",
    "purescript": "^0.15.4",
    "spago": "^0.20.9",
    "purescript-psa": "^0.8.2"
  },
  "scripts": {
    "build": "spago build --purs-args '--censor-codes=ImplicitImport --strict --stash'",
    "test": "spago test --purs-args '--censor-codes=ImplicitImport --strict --stash'",
    "serve": "parcel dev/index.html --open",
    "build-prod": "rm -rf prod && mkdir -p prod && cp dev/index.html prod/ && spago bundle-app --to prod/index.js && parcel build prod/index.html"
  }
}